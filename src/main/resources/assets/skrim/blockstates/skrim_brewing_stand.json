{
    "multipart": [
        {   "apply": { "model": "minecraft:brewing_stand" }},
        {   "when": { "skrim_has_bottle_0": true },
            "apply": { "model": "minecraft:brewing_stand_bottle0" }
        },
        {   "when": { "skrim_has_bottle_1": true },
            "apply": { "model": "minecraft:brewing_stand_bottle1" }
        },
        {   "when": { "skrim_has_bottle_2": true },
            "apply": { "model": "minecraft:brewing_stand_bottle2" }
        },
        {   "when": { "skrim_has_bottle_0": false },
            "apply": { "model": "minecraft:brewing_stand_empty0" }
        },
        {   "when": { "skrim_has_bottle_1": false },
            "apply": { "model": "minecraft:brewing_stand_empty1" }
        },
        {   "when": { "skrim_has_bottle_2": false },
            "apply": { "model": "minecraft:brewing_stand_empty2" }
        }
    ]
}
